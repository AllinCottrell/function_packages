set verbose off
include hausman_taylor.gfn
open nls_panel.gdt -q

list x1 = exper exper2 tenure tenure2 union
list x2 = south
list z1 = black
list z2 = educ

list L1 = x1 z1
list L2 = x2 z2

bht = hausman_taylor(lwage, L1, L2, 1)

# quit

# for comparison with stata (below)
series yh = bht.yhat
print yh --range=1:10

list L = id year lwage L1 L2
foreign language=stata --send-data=L
   xtset id year
   xthtaylor lwage exper exper2 tenure tenure2 union south black educ, endog(south educ) constant(black educ)
   predict yhat
   list yhat in 1/10
   estimates store eq_ht
   xtreg lwage exper exper2 tenure tenure2 union south, fe
   estimates store eq_fe
   hausman eq_fe eq_ht, df(4)
end foreign
