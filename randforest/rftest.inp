set verbose off
set R_functions on
include rfwip.inp
rf_setup()

open titanic3.gdt -q
set seed 877777
series sorter = uniform()
dataset sortby sorter

list L = survived pclass female sibsp parch fare embarked age
bundle opts = _(n_train=900, classify=1, seed=777778, tune=1, verbose=2)
bundle rfb = random_forest(L, opts)
print rfb
