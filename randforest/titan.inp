# tester, using inp directly rather than gfn

set R_functions on
include rfwip.inp
rf_setup()

open titanic.gdt
set seed 877777
series sorter = uniform()
dataset sortby sorter

list L = Survived Pclass Female SibSp Embarked Parch Fare
bundle opts = _(n_train=700, classify=1, seed=777778, tune=1, verbose=2)
bundle rfb = random_forest(L, opts)
